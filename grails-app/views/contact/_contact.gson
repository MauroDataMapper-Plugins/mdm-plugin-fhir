import uk.ac.ox.softeng.maurodatamapper.datamodel.DataModel

model {
    Map<String,Object> contactMap
}

log.debug("hello : ${binding.variables}")

json {

    if (contactMap.any {it.key.startsWith('contact.telecom')}){
        Map<String,Object> telecoms = contactMap.findAll {it.key.startsWith('contact.telecom')}
        telecom tmpl.'/contact/telecom'(telecoms)
    }

    name contactMap.find{it.key == 'contact.name'}.value
}